<section class="">
  <div class="bg-color-80 min-vh-100">
    <h1 class="ma0">{{service.name}}</h1>
    <h2>{{text.address}} {{service.Loc}}</h2>
    <ul class="pa0">
      <h3 class="">{{text.accessTitle}}</h3>
      {{#each service.accessOptions}}
      <li class="list">{{this}}</li>
      {{/each}}
    </ul>
    <p>{{text.desc}}</p><p>{{service.desc}}</p>
    <p>{{text.opens}}</p><p>{{service.open}}</p>
    <h2>{{text.contact}}</h2>
    <p>{{text.phone}}</p><p>{{service.phoneNumber}}</p>
    <p>{{text.email}}</p><p>{{service.email}}</p>
    <section class="f1">
      <a href="https://www.facebook.com"><i class="fa fa-facebook link dim white dib br-100 mr3" title='Link to Facebook Page'></i></a>
      <a href="https://www.instagram.com"><i class="fa fa-instagram link dim white dib br-100 mr3" title='Link to Instagram Page'></i></a>
    </section>
  </div>
</section>

{{> navbar}}
