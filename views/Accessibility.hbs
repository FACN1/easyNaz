       <input class="mr2" type="checkbox" id="Braille" value="Braille">
       <label for="Braille" class="lh-copy w3">{{text.braille}}</label>
     </section>
     <section class="tl mb2">
       <input class="mr2" type="checkbox" id=" Large Font Menus" value=" Large Font Menus">
